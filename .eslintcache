n(false);\r\n  }\r\n\r\n  function onConfirm() {\r\n    setIsConfirmPopupOpen(false)\r\n    setIsLoginPopupOpen(true);\r\n  }\r\n\r\n  function onRegister(evt) {\r\n    evt.preventDefault();\r\n    setIsRegisterPopupOpen(false);\r\n    setIsConfirmPopupOpen(true);\r\n  }\r\n\r\n  return (\r\n    <div className=\"App\">\r\n\r\n      <LoginPopup isOpen={isLoginPopupOpen} onClose={onClose} switchTo={switchTo} active={false} />\r\n\r\n      <RegisterPopup isOpen={isRegisterPopupOpen} onClose={onClose} onRegister={onRegister} switchTo={switchTo} active={true} />\r\n\r\n      <ConfirmPopup name=\"confirm\" isOpen={isConfirmPopupOpen} onClose={onClose} confirmed={onConfirm} title=\"Пользователь успешно зарегистрирован!\" undertext=\"Войти\" /> \r\n\r\n      <Header loggedIn={loggedIn} userName={userName} handleLogin={handleLogin} handleLogout={handleLogout} onAuth={handleLogin} isMobile={isMobile}/>\r\n\r\n      <Switch>\r\n        <Route exact path=\"/\">\r\n          \r\n          <Main articles={articles} loggedIn={loggedIn}/>\r\n\r\n        </Route>\r\n\r\n        <Route path=\"/saved-news\">\r\n          <SavedNewsHeader name={userName} articles={articles} />\r\n          <SavedNews articles={articles} />\r\n        </Route>\r\n\r\n      </Switch>\r\n\r\n      <Footer />\r\n        \r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\romas\\dev\\news-explorer-frontend\\src\\components\\Header\\Header.js",[],"C:\\Users\\romas\\dev\\news-explorer-frontend\\src\\components\\Main\\Main.js",[],"C:\\Users\\romas\\dev\\news-explorer-frontend\\src\\components\\SearchForm\\SearchForm.js",[],"C:\\Users\\romas\\dev\\news-explorer-frontend\\src\\components\\About\\About.js",[],"C:\\Users\\romas\\dev\\news-explorer-frontend\\src\\components\\Footer\\Footer.js",[],"C:\\Users\\romas\\dev\\news-explorer-frontend\\src\\components\\PopupWithForm\\PopupWithForm.js",[],"C:\\Users\\romas\\dev\\news-explorer-frontend\\src\\components\\NewsCard\\NewsCard.js",[],"C:\\Users